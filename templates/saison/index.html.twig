{% extends 'base.html.twig' %}

{% block body %}

        <h1 class="text-center mt-3">Recettes de saison</h1>
        <div class="mt-5 d-flex justify-content-center flex-wrap">
        {% for sai in saison %}
            <a href="saison/show/{{sai.id}}" ><div class="m-3 text-center"><i class="{{sai.thumbnail}}"></i><p class="mt-3">{{sai.nom}}</p></div></a>
        {% endfor %}
        </div>
{% endblock %}
